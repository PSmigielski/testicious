{
    "type": "object",
    "properties": {
        "password":{
            "type":"string"
        },
        "newPassword":{
            "type":"string",
            "pattern": "^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[#?!@$ %^&*-]).{8,100}$"
        }
    },
    "required": ["password","newPassword"],
    "additionalProperties": false
}